import time

def medir_tiempo(n):
    start_time = time.time()
    convertir_a_booleana(n) # type: ignore
    end_time = time.time()
    return end_time - start_time

tiempos = []
for i in range(1, 30001):
    tiempos.append(medir_tiempo(i))

for i in range(0, 30000, 5000):
    print(f"Tiempo para {i+1}: {tiempos[i]} segundos")
